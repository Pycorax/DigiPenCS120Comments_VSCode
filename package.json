{
	"name": "digipen-comments",
	"displayName": "DigiPen Comments",
	"version": "0.0.1",
	"publisher": "pycorax",
	"repository": {
		"type": "git",
		"url": "https://github.com/Pycorax/DigiPenCS120Comments_VSCode"
	},
	"bugs": {
		"url": "https://github.com/Pycorax/DigiPenCS120Comments_VSCode/issues"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"digipen",
		"comments",
		"autofill",
		"auto"
	],
	"preview": true,
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2",
		"@types/vscode": "^1.32.0"
	},
	"contributes": {
		"configuration": {
		  "title": "DigiPen Comments",
		  "properties": {
			"digipenComments.name": {
			  "type": "string",
			  "scope": "application",
			  "default": "@todo",
			  "description": "Your full name as per name lists."
			},
			"digipenComments.id": {
			  "type": "string",
			  "scope": "application",
			  "default": "@todo",
			  "description": "Your DigiPen ID as per your DigiPen Webmail address."
			},
			"digipenComments.section": {
			  "type": "string",
			  "scope": "application",
			  "default": "@todo",
			  "description": "Your DigiPen course section. Usually represented as a letter."
			},
			"digipenComments.course": {
			  "type": "string",
			  "scope": "application",
			  "default": "@todo",
			  "enum" : ["RTIS", "IMGD", "SEEMS"],
			  "enumDescriptions": [
				  "Real Time Interactive Simulations",
				  "Interactive Media and Game Design",
				  "Systems Engineering (ElectroMechanical Systems)"
			  ],
			  "description": "Your DigiPen course."
			}
		  }
		}
	  }
}
